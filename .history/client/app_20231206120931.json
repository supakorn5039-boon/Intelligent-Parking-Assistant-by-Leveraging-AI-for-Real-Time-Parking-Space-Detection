{
  "expo": {
    "name": "Smart-Parking",
    "slug": "Smart-Parking",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000"
    },
    "dependencies" : {
      "@react-native-firebase/analytics": "^17.3.1",
      "@react-native-firebase/app": "^17.3.2",
      "@react-native-firebase/remote-config": "^17.3.1",
  }
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#ffffff"
      }
    },
    "expo": {
      "android": {
        "useNextLegacyApi": true
      }
    },
    "web": {
      "favicon": "./assets/images/splash.png"
    }
  }
}
